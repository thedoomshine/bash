{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        "build/**",
        "api/**",
        "public/build/**"
      ]
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "cleanup-preview": {
      "outputs": []
    },
    "deploy": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "format:check": {
      "outputs": []
    },
    "format:write": {
      "outputs": []
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "outputs": []
    },
    "lint": {
      "dependsOn": [
        "format:check"
      ],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": [
        "format:write"
      ],
      "outputs": []
    },
    "lint:report": {
      "dependsOn": [
        "format:check"
      ],
      "outputs": [
        "lint-results/**"
      ]
    },
    "preview": {
      "outputs": []
    },
    "start": {
      "outputs": []
    },
    "supabase#lint": {
      "dependsOn": [
        "supabase#start",
        "format:check"
      ],
      "outputs": []
    },
    "supabase#lint:report": {
      "dependsOn": [
        "supabase#start",
        "format:check"
      ],
      "outputs": []
    },
    "supabase#start": {
      "cache": false,
      "outputs": []
    },
    "supabase#test": {
      "dependsOn": [
        "supabase#start",
        "^build"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "types#generate": {
      "dependsOn": [
        "supabase#start",
        "^generate"
      ],
      "outputs": []
    }
  }
}
