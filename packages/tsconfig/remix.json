{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Remix",
  "include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx"],
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ES2020", "ES2020.Intl", "ESNext"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ESNext"
  }
}
