{
  "name": "pages",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build:swc -w",
    "build": "rm -rf dist && pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf ./dist && rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.0 || ^18.0.0-0",
    "react-dom": "^17.0.0 || ^18.0.0-0"
  },
  "dependencies": {
    "ui": "workspace:*",
    "@swc/helpers": "latest"
  },
  "devDependencies": {
    "@swc/cli": "latest",
    "@swc/core": "latest",
    "@types/react": "^18.0.37",
    "chokidar": "latest",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.2",
    "@stitches/react": "^1.2.8"
  }
}